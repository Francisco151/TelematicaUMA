#include <iostream>
using namespace std;
int main (){
 char letra1;
 char letra2;
 cout << "Introduce la letra1";
 cin >> letra1;

 cout << "Introduce la letra2";
 cin >> letra2;

 if (letra1 < letra2)
 cout << "Las letras están ordenadas" ;

 if (letra1 > letra2)
 cout << "Las letras no están ordenadas" ;
return 0;
}
